{
  "name": "moj-frontend",
  "description": "The MOJ Frontend contains the code you need to start building user interfaces for UK Ministry of Justice government services.",
  "main": "app.js",
  "workspaces": [
    "/package"
  ],
  "files": [
    "/package"
  ],
  "scripts": {
    "build:docs": "gulp build:docs && eleventy --input=./docs --output=public",
    "build:dist": "gulp build:dist",
    "build:package": "gulp build:package",
    "postbuild:package": "govuk-prototype-kit validate-plugin package",
    "ci:release": "CI=true semantic-release --debug",
    "ci:dryrun": "CI=true semantic-release --dry-run",
    "start": "npm-run-all --parallel watch:*",
    "test": "npm-run-all --parallel test:*",
    "test:docs": "npm run build:docs",
    "test:js": "jest src/",
    "test:sass": "sass --color --load-path . --quiet-deps package/moj/all.scss > /dev/null && echo \"ok\"",
    "lint": "npm run lint:prettier && npm run lint:js && npm run lint:scss",
    "lint:js": "npm run lint:js:cli -- \"**/*.{cjs,js,md,mjs}\"",
    "lint:js:cli": "eslint --cache --cache-location .cache/eslint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0",
    "lint:prettier": "npm run lint:prettier:cli -- \"**/*.{cjs,js,json,md,mjs,scss,yaml,yml}\"",
    "lint:prettier:cli": "prettier --cache --cache-location .cache/prettier --cache-strategy content --check",
    "lint:scss": "npm run lint:scss:cli -- \"**/*.{md,scss}\"",
    "lint:scss:cli": "stylelint --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0",
    "watch:11ty": "ENV=dev eleventy --input=./docs --output=public --serve",
    "watch:package": "ENV=dev gulp watch:dev",
    "watch:app": "nodemon app.js",
    "start:eleventy": "ENV=dev eleventy --input=./docs --output=public --watch",
    "start:server": "nodemon app.js",
    "dev": "concurrently \"npm run start:eleventy\" \"npm run start:server\"",
    "test:helpers": "jest helpers/",
    "test:middleware": "jest middleware/",
    "test:routes": "jest routes/",
    "dev:docker:preview:build": "docker-compose -f docker-compose.preview.yml build --no-cache",
    "dev:docker:preview:up": "docker-compose -f docker-compose.preview.yml up --build",
    "dev:docker:preview:down": "docker-compose -f docker-compose.preview.yml down"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ministryofjustice/moj-frontend.git"
  },
  "author": "MOJ Digital & Technology",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "private": false,
  "bugs": {
    "url": "https://github.com/ministryofjustice/moj-frontend/issues"
  },
  "homepage": "https://github.com/ministryofjustice/moj-frontend#readme",
  "dependencies": {
    "@ministryofjustice/frontend": "*",
    "connect-redis": "^6.1.3",
    "express": "^4.21.2",
    "express-nunjucks": "^3.1.2",
    "express-session": "^1.17.3",
    "govuk-frontend": "^5.4.1",
    "ioredis": "^5.5.0",
    "joi": "^17.13.3",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "2.7.0",
    "notifications-node-client": "^8.2.1",
    "nunjucks": "^3.2.3",
    "sanitize-html": "^2.14.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^29.7.0",
    "cheerio": "^1.0.0",
    "clipboard": "^2.0.8",
    "commitizen": "^4.2.5",
    "concurrently": "^9.1.2",
    "cssnano": "^7.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.13",
    "del": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-es-x": "^8.4.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "glob": "^11.0.1",
    "govuk-prototype-kit": "^13.16.2",
    "gray-matter": "^4.0.3",
    "gulp": "^5.0.0",
    "gulp-cache": "^1.1.3",
    "gulp-imagemin": "^9.1.0",
    "gulp-rev": "^11.0.0",
    "gulp-zip": "^6.1.0",
    "highlight.js": "^11.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^11.0.1",
    "jest": "^29.2.2",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-sinon": "^1.1.0",
    "jquery": "^3.7.1",
    "js-beautify": "^1.13.13",
    "lodash": "^4.17.21",
    "markdown-it": "^13.0.2",
    "markdown-it-anchor": "^8.6.7",
    "mock-match-media": "^0.4.3",
    "nodemon": "^3.1.9",
    "npm-run-all2": "^6.0.0",
    "nunjucks-async-loader": "^2.1.3",
    "postcss": "^8.4.31",
    "postcss-markdown": "^1.3.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.4.2",
    "require-dir": "^1.2.0",
    "rollup": "^4.34.4",
    "rollup-plugin-external-globals": "^0.13.0",
    "sass-embedded": "^1.83.4",
    "semantic-release": "^23.0.0",
    "semantic-release-plugin-update-version-in-files": "^1.1.0",
    "sinon": "^19.0.2",
    "stylelint": "^16.13.2",
    "stylelint-config-gds": "^2.0.0",
    "stylelint-order": "^6.0.4",
    "supertest": "^7.0.0",
    "terser": "^5.38.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
