{
  "name": "moj-frontend",
  "description": "The MOJ Frontend contains the code you need to start building user interfaces for UK Ministry of Justice government services.",
  "main": "app.js",
  "workspaces": [
    "/package"
  ],
  "files": [
    "/package"
  ],
  "scripts": {
    "prebuild": "npm run build:package",
    "build": "concurrently npm:build:dist npm:build:docs --group --prefix none",
    "build:docs": "gulp build:docs --color && eleventy --input=./docs --output=public",
    "build:dist": "gulp build:dist --color",
    "build:package": "gulp build:package --color",
    "postbuild:package": "govuk-prototype-kit validate-plugin package",
    "ci:release": "CI=true semantic-release --debug",
    "ci:dryrun": "CI=true semantic-release --debug --dry-run --no-ci",
    "prestart": "ENV=development npm run build:package && gulp build:docs",
    "start": "concurrently npm:watch:* --prefix none",
    "start:e2e": "ENV=test nodemon --delay 1000ms app/app.js",
    "test": "concurrently npm:test:* --group --prefix none",
    "test:e2e": "npx playwright test",
    "test:components": "jest --color src/",
    "test:helpers": "jest --color helpers/",
    "test:middleware": "jest --color middleware/",
    "test:scss": "sass --color --load-path . --quiet-deps src/moj/all.scss > /dev/null && echo \"ok\"",
    "lint": "npm run lint:types && npm run lint:js && npm run lint:scss && npm run lint:prettier",
    "lint:fix": "npm run lint:types && npm run lint:js:fix && npm run lint:scss:fix && npm run lint:prettier:fix",
    "lint:js": "npm run lint:js:cli -- \"**/*.{cjs,js,md,mjs}\"",
    "lint:js:fix": "npm run lint:js:cli -- --fix \"**/*.{cjs,js,md,mjs}\"",
    "lint:js:cli": "eslint --cache --cache-location .cache/eslint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0",
    "lint:prettier": "npm run lint:prettier:cli -- \"**/*.{cjs,js,json,mjs,scss,yaml,yml}\"",
    "lint:prettier:fix": "npm run lint:prettier:cli -- --write \"**/*.{cjs,js,json,mjs,scss,yaml,yml}\"",
    "lint:prettier:cli": "prettier --cache --cache-location .cache/prettier --cache-strategy content --check",
    "lint:scss": "npm run lint:scss:cli -- \"**/*.scss\"",
    "lint:scss:fix": "npm run lint:scss:cli -- --fix \"**/*.scss\"",
    "lint:scss:cli": "stylelint --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore --max-warnings 0",
    "lint:types": "tsc --build tsconfig.json --incremental --pretty",
    "watch:11ty": "eleventy --input=./docs --output=public --watch",
    "watch:package": "gulp watch --color",
    "watch:app": "nodemon --delay 1000ms app/app.js",
    "dev:docker:preview:build": "docker-compose -f docker-compose.preview.yml build --no-cache",
    "dev:docker:preview:up": "docker-compose -f docker-compose.preview.yml up --build",
    "dev:docker:preview:down": "docker-compose -f docker-compose.preview.yml down"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ministryofjustice/moj-frontend.git"
  },
  "author": "MOJ Digital & Technology",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "private": false,
  "bugs": {
    "url": "https://github.com/ministryofjustice/moj-frontend/issues"
  },
  "homepage": "https://github.com/ministryofjustice/moj-frontend#readme",
  "dependencies": {
    "@ministryofjustice/frontend": "*",
    "@sentry/node": "^9.46.0",
    "@sentry/rollup-plugin": "^3.6.1",
    "connect-redis": "^6.1.3",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-nunjucks": "^3.1.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-xss-sanitizer": "^2.0.0",
    "govuk-frontend": "^5.11.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "joi": "^17.13.3",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "node-fetch": "2.7.0",
    "notifications-node-client": "^8.2.1",
    "nunjucks": "^3.2.4",
    "sanitize-filename": "^1.6.3",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.0",
    "@11ty/eleventy-navigation": "^1.0.4",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/connect-redis": "^0.0.23",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/govuk-frontend": "^5.9.0",
    "@types/gulp": "^4.0.17",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/jest-sinon": "^1.0.5",
    "@types/nise": "^1.4.5",
    "@types/node": "^22.13.10",
    "@types/nunjucks": "^3.2.6",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "clipboard": "^2.0.11",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.13",
    "del": "^8.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-es-x": "^8.7.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^50.8.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "glob": "^11.0.2",
    "govuk-prototype-kit": "^13.16.2",
    "gray-matter": "^4.0.3",
    "gulp": "^5.0.1",
    "gulp-cache": "^1.1.3",
    "gulp-filter": "^9.0.1",
    "gulp-imagemin": "^9.1.0",
    "gulp-rev": "^11.0.0",
    "gulp-zip": "^6.1.0",
    "highlight.js": "^11.11.1",
    "ioredis-mock": "^8.9.0",
    "jest": "^30.1.3",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-sinon": "^1.1.0",
    "js-beautify": "^1.15.4",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "mock-match-media": "^1.0.2",
    "nise": "^6.1.1",
    "nodemon": "^3.1.10",
    "outdent": "^0.8.0",
    "plugin-error": "^2.0.1",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "postcss-markdown": "^1.3.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "puppeteer": "^24.22.0",
    "puppeteer-core": "^24.22.0",
    "require-dir": "^1.2.0",
    "rollup": "^4.50.2",
    "sass-embedded": "^1.92.1",
    "semantic-release": "^24.2.8",
    "sinon": "^21.0.0",
    "stylelint": "^16.24.0",
    "stylelint-config-gds": "^2.0.0",
    "stylelint-order": "^6.0.4",
    "supertest": "^7.1.1",
    "terser": "^5.44.0",
    "typed-query-selector": "^2.12.0",
    "typescript": "^5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.50.2",
    "sass-embedded-linux-x64": "^1.87.0"
  }
}
