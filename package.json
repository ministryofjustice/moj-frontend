    "dayjs": "^1.11.13",
    "del": "^7.0.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "gulp-postcss": "^10.0.0",
